@helper simpleInputField(string type, string label, string id, string name, string required, string value)
{
	<div>
	<label><strong>@label</strong></label>
	@if(type == "number"){
	<input type="@type" id="@id" name="@name" @required value="@value" step="0.00001"></input>
    }
	else
	{
	<input type="@type" id="@id" name="@name" @required value="@value"></input>
	}
    </div>
} 
@helper sinpleCheckboxField(string label, string id, string name, string value, string isChecked )
{
    var showAsChecked = isChecked == "True" ? "checked" : "";
    
    
    <div style="overflow: hidden">
        <label style="display: inline-block; float: left"><strong>@label</strong></label>
        <input style="display: inline-block; float: left; margin-left: 10px; margin-top: 5px; width: initial;" type="checkbox" id="@id" name="@name" value="@value" @showAsChecked></input>
    </div>
}
@helper simpleTextAreaField(string label, string id, string name, string required, string value)
{
    <div>
        <label><strong>@label</strong></label>
        <textarea id="@id" name="@name" cols="100" rows="10" @required >@value</textarea>
    </div>
}
@helper preBuiltEmlement (string label, string cssClass, string systemName)
{
    <div>
        <label><strong>@label</strong></label>
       @systemName
    </div>
}
