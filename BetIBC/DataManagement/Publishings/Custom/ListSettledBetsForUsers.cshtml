@{
	var ViewId = Pageview.CurrentParagraph.ModuleProperties.get_Value("ViewID"); 
    var ClientId = GetGlobalValue("Global:Extranet.UserID") != null ? GetGlobalValue("Global:Extranet.UserID").ToString() : "No User.";
    var secondaryUserRaw = Pageview.User.ID;
    var secondaryUser = secondaryUserRaw != null ? secondaryUserRaw.ToString() : "No Secondary User.";
}
     
<div class="form_wrapper" style="width: 100%;">
            <div class="container">
