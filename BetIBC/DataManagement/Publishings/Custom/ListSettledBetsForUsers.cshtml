                <table class="main_table crosscheck-bet">
                    <thead>
                        <tr>
                            <th> ID</th>
                            <th>SELECTION </th>
                            <th> BET DETAILS </th>
                            <th> ODD<BR> min | bet | give</th>
                            <th> STAKE</th>
                            <th>
                                COMISSION <br>
                                % | EUR
                            </th>
                            <th> EARNING EUR</th>
                            <th> OPERATORS </th>
                              <th> DETAILS</th>
                        </tr>
                    </thead>
                    <tbody id="all-bets">
 						@foreach (LoopItem row in GetLoop("Rows"))
                   		{
                      		var rowId = row.GetInteger("skypeBetsID");
                      		var rowName = "skypeBetsID";
                      		var paramerter = "?FORM_ROW_ID=" + rowName + "%3d" + rowId + "&VIEW_ID=" + ViewId;
                            var parameterDetails = "mode=detail&RowId=skypeBetsID%3d" + rowId + "&ViewPID=5151";
                            var processingBetStatus = row.GetValue("skypeBets_ProcessingBetStatus").ToString();
                            var BetClientId = row.GetValue("skypeBets_SkypeUserID") != null ? row.GetValue("skypeBets_SkypeUserID").ToString() : "";
                            var detailsUrl = row.GetValue("DetailUrl");

                      		if (processingBetStatus == "Crosschecked") {
                      					if ( ClientId == BetClientId || secondaryUser == BetClientId ) {
                      
                      
                        <tr> 
                            <td><div class="p-10">@row.GetValue("skypeBetsID")
                                <div>@row.GetValue("skypeBets_BetIdInAsia")</div>
                              	<div><i class="fa fa-info-circle" aria-hidden="true" data-placement="bottom" data-toggle="tooltip" title='@row.GetValue("skypeBets_BetNotes")'></i></div>
                              	</div>
                            </td>
                            <td><div class="p-10">
                                    <div>@row.GetValue("skypeBets_SoccerFinalTimeHalfTime")</div>
                                    <div>@row.GetValue("skypeBets_SoccerBetType")</div>
                                    <div>@row.GetValue("skypeBets_LineOrHC")</div>
                                    <div>@row.GetValue("skypeBets_SoccerTeamToBet")</div>
                                    <div>@row.GetValue("skypeBets_SoccerHCType")</div>
                                    <div>@row.GetValue("skypeBets_Market")</div>
                                </div>
                            </td>
                            <td><div class="p-10">
                                    <div>@row.GetValue("skypeBets_BetLeague")</div>
                                    <div>@row.GetValue("skypeBets_BetMatch")</div>
                                    <div>@row.GetValue("skypeBets_BetPrematchOrRunning")</div>
                              		<div><span><small>Score bet placement:</small></span>  @row.GetValue("skypeBets_ScoreAtBetPlacement")</div>
                                    <div>@row.GetValue("skypeBets_GameEndTime")</div>
                              		<div>@row.GetValue("skypeBets_TimeZone")</div>
                              		<div>HT: @row.GetValue("skypeBets_ResultAtHT")</div>
                              		<div>FT: @row.GetValue("skypeBets_ResultAtFT")</div>
                              		<div><span><small>Result:</small></span>  @row.GetValue("skypeBets_BetResult")</div>
                                </div>
                            </td>
                            <td>
                                <div class="col-xs-12 odd_item lh">
                                    @row.GetValue("skypeBets_MinimumOddRequestedByClient")
                                </div>
                                <div class="col-xs-12 odd_item lh">
                                    @row.GetValue("skypeBets_OddWeGiveToClient")
                                </div>
                                <div class="col-xs-12 odd_item no--border lh">
                                    @row.GetValue("skypeBets_OddWeBetInAsia")
                                </div>
                            </td>
                            <td>
                                @row.GetValue("skypeBets_BetStake")
                            </td>
                            <td>
                                <div class="col-xs-12 com-odd_input lh">
                                    @row.GetValue("skypeBets_ComissionToDisplay")	
                                </div>
                                <div class="col-xs-12 com-odd_input no--border lh">
                                   	@row.GetValue("skypeBets_ComissionAmount") 
                                </div>
                            </td>
                            <td>
                                  	@row.GetValue("skypeBets_MemberProfit")<br>
                                  	@row.GetValue("skypeBets_BetCurrency")
                            </td>
                            <td class="target-dates" dates='@row.GetValue("skypeBets_AddNewDateTime") , @row.GetValue("skypeBets_ConfirmationNewDateTime") , @row.GetValue("skypeBets_SettlementNewDateTime") , @row.GetValue("skypeBets_CrossCheckedNewDateTime")'><div class="p-10">
                                    <div>1.@row.GetValue("skypeBets_FirstAdminUsername")
                                        <div class="date--time">@row.GetValue("skypeBets_AddNewDateTime")</div>
                                    </div>
                                    <div>2.@row.GetValue("skypeBets_SecondAdminUsername")
                                        <div class="date--time">@row.GetValue("skypeBets_ConfirmationNewDateTime")</div>   
                                    </div>
                                    <div>3.@row.GetValue("skypeBets_ThirdAdminUsername")
                                        <div class="date--time">@row.GetValue("skypeBets_SettlementNewDateTime")</div>
                                    </div>
                                    <div>4.@row.GetValue("skypeBets_FourthAdminUsername") 
                                        <div class="date--time_last">@row.GetValue("skypeBets_CrossCheckedNewDateTime")</div>
                                    </div>
                                </div>
                            </td>
                              <td><div class="p-10"><a class="table-link" href="/english/skype-betting/settled?@(parameterDetails)">SEE DETAILS</a></div></td>
                        </tr>
                             }
                         }
                        }
                    </tbody>
                </table> 
                
 
            </div>
