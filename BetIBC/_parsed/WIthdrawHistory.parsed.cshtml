	var allValidAccontsjson ="/Default.aspx?ID=" + GetPageIdByNavigationTag("AllValidAcconts").ToString();
	var depositsHistoryjson ="/Default.aspx?ID=" + GetPageIdByNavigationTag("DepositsHistoryFeed").ToString();
    var withdrawHistoryjson ="/Default.aspx?ID=" + GetPageIdByNavigationTag("WithdrawHistoryFeed").ToString();
    var transferHistoryjson ="/Default.aspx?ID=" + GetPageIdByNavigationTag("TransferHistoryFeed").ToString();
    var tipsterFeedByLanguage ="/Default.aspx?ID=" + GetPageIdByNavigationTag("TipstersFeedDetailsByLanguage").ToString();
    var newPredictionsFeed = "/Default.aspx?ID=" + GetPageIdByNavigationTag("NewPredictionsByLanguage").ToString();
    var bookmakersFeed = "/Default.aspx?ID=" + GetPageIdByNavigationTag("BookmakersOpenAccount").ToString();
    var countPredictionsFeed = "/Default.aspx?ID=" + GetPageIdByNavigationTag("PredictionCount").ToString();
