@helper RenderMasterMetadata() { 
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="robots" content="index, follow">
if (!Model.MetaTags.Contains("og:image")) {
        Pageview.Meta.AddTag("og:image", string.Format("{0}://{1}{2}", Dynamicweb.Context.Current.Request.Url.Scheme, HttpContext.Current.Request.Url.Host, Model.PropertyItem.GetFile("OpenGraphImage")));
        <meta name="image" property="og:image" content='@string.Format("{0}://{1}{2}", Dynamicweb.Context.Current.Request.Url.Scheme, HttpContext.Current.Request.Url.Host, Model.PropertyItem.GetFile("OpenGraphImage"))'>
        <meta name="image:alt" property="og:image:alt"  content="@Model.Title">
    }
    @Model.MetaTags
}

