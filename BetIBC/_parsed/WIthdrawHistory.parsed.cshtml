    var login = "/Default.aspx?ID=" + GetPageIdByNavigationTag("SignInProfile").ToString();
    var allExceptionforUser = "/Default.aspx?ID=" + GetPageIdByNavigationTag("ExceptionsFeedFilterdById").ToString();
	var EwalletComissionsjson ="/Default.aspx?ID=" + GetPageIdByNavigationTag("EwalletCommissionsFeed").ToString();
    var BookmakersComissionsjson ="/Default.aspx?ID=" + GetPageIdByNavigationTag("BookmakersCommissionsFeed").ToString();
