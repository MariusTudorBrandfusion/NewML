                                            if (!megaMenu)
                                            {
                                            @RenderNavigation(new
                                       {
                                           id = "topnavigation",
                                           cssclass = "menu dw-mod dwnavigation",
                                           startLevel = 1,
                                           endlevel = 4,
                                           expandmode = "All",
                                           template = "BaseMenuWithDropdown.xslt"
                                       });
